{
  "name": "isbn-bisac-tools",
  "version": "1.0.0",
  "description": "A toolkit for working with BISAC subject headings and ISBN lookups",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "tsx src/index.ts",
    "scrape:url": "tsx src/index.ts --url",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:bisac": "tsx test/bisac-selection.script.ts",
    "lint": "eslint \"src/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\" --fix",
    "format": "prettier --write 'src/**/*.ts' 'lib/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'lib/**/*.ts' 'test/**/*.ts'",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "browse:json": "tsx src/browse-json.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "scraper",
    "puppeteer",
    "bisac",
    "headless-browser"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "glob": "^11.0.2",
    "inquirer": "9.2.17",
    "ora": "^8.2.0",
    "puppeteer": "^24.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.10.5",
    "@types/ora": "^3.1.0",
    "@types/puppeteer": "^7.0.4",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "fx": "^31.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^7.0.2",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
